

def recur_test(v:int) -> int:
    if (v <= 2 or v >= 15):
        return 1

    let minus:int = v - 1
    print('m', 'b')
    print(minus)
    minus = recur_test(minus)
    print('m', 'a')
    print(minus)


    let plus: int = v  + 1
    print('p', 'b')
    print(plus)
    minus = recur_test(plus)
    print('p', 'a')
    print(plus)


    return minus 

recur_test(7)