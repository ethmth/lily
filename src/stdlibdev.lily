# sorting functions

def quicksort(l::int) -> int list:
    if (len(l) <= 1):
        return l
    let pivot:int = l[len(l) / 2]
    let less::int = []
    let equal::int = []
    let greater::int = []
    for num in l:
        if (num < pivot):
           append(less, num)
        if (num == pivot):
            append(equal, num)
        else:
            append(greater, num)    
    return concatenate(quicksort(less), equal, quicksort(greater))
