def concatenate(a::int, b::int) -> int list:
    let result::int = []
    for num in a:
        result.append(num)
    for num in b:
        result.append(num)
    return result

def concatenate3(a::int, b::int, c::int) -> int list:
    let result::int = []
    for num in a:
        result.append(num)
    for num in b:
        result.append(num)
    for num in c:
        result.append(num)
    return result